{
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "name": "gateway",
  "projectType": "library",
  "root": "omf3/libs/gateway",
  "sourceRoot": "omf3/libs/gateway/src",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/gateway",
        "tsConfig": "omf3/libs/gateway/tsconfig.lib.json",
        "main": "omf3/libs/gateway/src/index.ts",
        "assets": [],
        "rootDir": "."
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tsx omf3/libs/gateway/src/__tests__/gateway.spec.ts"
      }
    }
  },
  "tags": ["scope:shared", "type:lib"]
}

