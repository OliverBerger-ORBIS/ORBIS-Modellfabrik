{
  "$schema": "./node_modules/nx/schemas/nx-schema.json",
  "npmScope": "omf3",
  "workspaceLayout": {
    "appsDir": "omf3/apps",
    "libsDir": "omf3/libs"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "nx/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test"]
      }
    }
  },
  "targetDefaults": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["{options.outputPath}"]
    }
  }
}

