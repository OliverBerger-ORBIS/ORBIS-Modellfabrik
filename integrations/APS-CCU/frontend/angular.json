{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "ff-frontend": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        },
        "@schematics/angular:application": {
          "strict": true
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "i18n": {
        "locales": {
          "de": {
            "translation": "src/assets/i18n/messages-ft.de.xlf",
            "baseHref": "/de/aps/"
          },
          "en": {
            "translation": "src/assets/i18n/messages-ft.en.xlf",
            "baseHref": "/en/aps/"
          },
          "es": {
            "translation": "src/assets/i18n/messages-ft.es.xlf",
            "baseHref": "/es/aps/"
          },
          "fr": {
            "translation": "src/assets/i18n/messages-ft.fr.xlf",
            "baseHref": "/fr/aps/"
          },
          "nl": {
            "translation": "src/assets/i18n/messages-ft.nl.xlf",
            "baseHref": "/nl/aps/"
          },
          "pt": {
            "translation": "src/assets/i18n/messages-ft.pt.xlf",
            "baseHref": "/pt/aps/"
          },
          "ru": {
            "translation": "src/assets/i18n/messages-ft.ru.xlf",
            "baseHref": "/ru/aps/"
          }
        }
      },
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/ff-frontend",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "localize": true,
            "i18nMissingTranslation": "warning",
            "assets": [
              "src/favicon.ico",
              "src/assets",
              {
                "glob": "**/*.*",
                "input": "src/assets",
                "output": "assets"
              },
              {
                "glob": "**/*.*",
                "input": "node_modules/@ft/futurefactory/assets",
                "output": "assets"
              },
              {
                "glob": "**/*.*",
                "input": "node_modules/@fischertechnik/ft-window/assets",
                "output": "assets"
              },
              {
                "glob": "**/*.*",
                "input": "node_modules/@fischertechnik/ft-common-ui/assets/i18n",
                "output": "assets/i18n"
              }
            ],
            "styles": [
              "src/styles.scss"
            ],
            "scripts": [],
            "allowedCommonJsDependencies": [
              "mqtt-browser",
              "fast-xml-parser",
              "@xmldom/xmldom"
            ]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "5mb",
                  "maximumError": "7mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "10kb",
                  "maximumError": "20kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "aot": true,
              "buildOptimizer": true,
              "optimization": true,
              "vendorChunk": true,
              "extractLicenses": true,
              "sourceMap": true,
              "namedChunks": true,
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true,
              "localize": [
                "de"
              ],
              "baseHref": "/"
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "ff-frontend:build:production"
            },
            "development": {
              "browserTarget": "ff-frontend:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "ff-frontend:build"
          }
        },
        "test": {
          "builder": "@angular-builders/jest:run",
          "options": {
            "no-cache": true,
            "reporters": [
              "default",
              "jest-junit"
            ],
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "src/**/*.ts",
              "src/**/*.html"
            ]
          }
        }
      }
    },
    "futurefactory": {
      "projectType": "library",
      "root": "projects/futurefactory",
      "sourceRoot": "projects/futurefactory/src",
      "prefix": "ff",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/futurefactory/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/futurefactory/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/futurefactory/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-builders/jest:run",
          "options": {
            "no-cache": true,
            "reporters": [
              "default",
              "jest-junit"
            ],
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "projects/futurefactory/**/*.ts",
              "projects/futurefactory/**/*.html"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "ff-frontend",
  "cli": {
    "analytics": false,
    "defaultCollection": "@angular-eslint/schematics"
  }
}