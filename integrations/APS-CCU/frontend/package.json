{
  "name": "ff-frontend",
  "version": "1.3.0",
  "scripts": {
    "ng": "ng",
    "copy:common": "node ./copy-common.js",
    "create:version": "node ./create-version-file.js",
    "postinstall": "npm run copy:common",
    "prestart": "npm run copy:common",
    "start": "npm run create:version && ng serve --host 0.0.0.0 --poll=2000",
    "prebuild": "npm run copy:common",
    "build": "npm run create:version && ng build --configuration production",
    "build:futurefactory": "npm run copy:common && npm run create:version && ng build --project futurefactory --configuration production",
    "watch": "ng build --watch --configuration development",
    "pretest": "npm run copy:common",
    "test": "ng test",
    "lint": "ng lint",
    "i18n:extract": "ng extract-i18n --format=xlf --out-file=src/assets/i18n/messages.xlf",
    "i18n:extract:futurefactory": "ngx-translate-extract --input ./projects --source-language=de --output ./projects/futurefactory/assets/i18n/messages-omm.{de,en,es,fr,nl,pt,ru}.xlf --format xlf"
  },
  "private": true,
  "engines": {
    "node": ">= 18.16.0 < 19.0.0",
    "npm": ">= 9.5.1"
  },
  "dependencies": {
    "@angular/animations": "13.4.0",
    "@angular/cdk": "13.3.9",
    "@angular/common": "13.4.0",
    "@angular/core": "13.4.0",
    "@angular/forms": "13.4.0",
    "@angular/material": "13.3.9",
    "@angular/platform-browser": "13.4.0",
    "@angular/platform-browser-dynamic": "13.4.0",
    "@angular/router": "13.4.0",
    "@beemo/oauth2": "file:./packages/beemo-oauth2-13.0.0.tgz",
    "@fischertechnik/ft-api": "file:./packages/ft-api-13.3.15.tgz",
    "@fischertechnik/ft-mqtt": "file:./packages/ft-mqtt-13.3.8.tgz",
    "@fischertechnik/ft-window": "file:./packages/ft-window-13.8.11.tgz",
    "@ngx-translate/core": "^13.0.0",
    "@ngx-translate/http-loader": "^6.0.0",
    "angular-gridster2": "^13.3.2",
    "fast-xml-parser": "^4.2.4",
    "ng-mocks": "^14.6.0",
    "ngx-mqtt": "^9.0.5",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4",
    "@beemo/ngx-gallery": "file:./packages/beemo-ngx-gallery-13.0.0.tgz",
    "@beemo/ngx-i18nsupport-lib": "file:./packages/beemo-ngx-i18nsupport-lib-13.0.1.tgz",
    "@fischertechnik/ft-common-ui": "file:./packages/ft-common-ui-13.2.5.tgz",
    "@fischertechnik/ft-environment": "file:./packages/ft-environment-13.2.5.tgz"
  },
  "devDependencies": {
    "@angular-builders/jest": "~13.0.4",
    "@angular-devkit/build-angular": "~13.3.11",
    "@angular-eslint/builder": "~13.5.0",
    "@angular-eslint/eslint-plugin": "~13.5.0",
    "@angular-eslint/eslint-plugin-template": "~13.5.0",
    "@angular-eslint/schematics": "~13.5.0",
    "@angular-eslint/template-parser": "~13.5.0",
    "@angular/cli": "~13.3.11",
    "@angular/compiler-cli": "~13.4.0",
    "@omm/ngx-translate-extract": "file:./packages/omm-ngx-translate-extract-8.0.4.tgz",
    "@types/d3-scale": "^4.0.3",
    "@types/d3-selection": "^3.0.5",
    "@types/d3-shape": "^3.1.1",
    "@types/jest": "^29.4.0",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "5.27.1",
    "@typescript-eslint/parser": "5.27.1",
    "eslint": "^8.17.0",
    "fs-extra": "^11.1.1",
    "jest": "^28.1.3",
    "jest-junit": "^15.0.0",
    "jest-preset-angular": "^12.2.6",
    "ng-packagr": "~13.3.1",
    "typescript": "~4.6.2"
  }
}
