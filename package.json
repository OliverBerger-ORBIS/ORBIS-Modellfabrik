{
  "name": "omf3-workspace",
  "version": "0.5.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "nx": "nx",
    "graph": "nx graph",
    "test": "nx run-many -t test",
    "test:osf-ui": "nx test osf-ui",
    "test:mqtt-client": "nx test mqtt-client",
    "test:coverage": "nx test osf-ui --coverage",
    "test:coverage:check": "nx test osf-ui --coverage --coverageThreshold",
    "build": "nx build mqtt-client",
    "build:fixtures": "python scripts/build_order_fixtures.py",
    "build:github-pages": "nx build osf-ui --configuration=github-pages",
    "serve:local": "npm run build:github-pages && npx serve dist/apps/osf-ui/browser -p 4200",
    "version": "npm run build && git add -A",
    "postversion": "git push && git push --tags",
    "version:patch": "npm version patch -m 'chore(release): %s'",
    "version:minor": "npm version minor -m 'feat(release): %s'",
    "version:major": "npm version major -m 'feat(release): %s [BREAKING]'",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~18.2.0",
