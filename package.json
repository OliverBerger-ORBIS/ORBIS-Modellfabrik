    "@angular/language-service": "~18.2.0",
    "@eslint/js": "^9.8.0",
    "@nx/angular": "19.8.5",
    "@nx/devkit": "19.8.5",
    "@nx/eslint": "19.8.5",
    "@nx/eslint-plugin": "19.8.5",
    "@nx/jest": "19.8.5",
    "@nx/js": "19.8.5",
    "@nx/web": "19.8.5",
    "@nx/workspace": "19.8.5",
    "@schematics/angular": "~18.2.0",
    "@swc-node/register": "~1.9.1",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "@types/highlight.js": "^9.12.4",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.16.5",
    "@typescript-eslint/utils": "^8.0.0",
    "angular-eslint": "^18.3.0",
    "conventional-changelog-cli": "^4.1.0",
    "eslint": "^9.8.0",
    "eslint-config-prettier": "^9.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-preset-angular": "~14.1.0",
    "mqtt": "^5.14.1",
    "nx": "19.8.5",
    "prettier": "^2.6.2",
    "ts-jest": "^29.1.0",
